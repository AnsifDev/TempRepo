<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="gitlink">
	<schema id="com.asiet.lab.GitLink" path="/com/asiet/lab/GitLink/">
		<key name="app-mode" type="s">
			<default>'unknown'</default>
			<choices>
				<choice value='personal'/>
				<choice value='lab-system'/>
				<choice value='lab-host'/>
				<choice value='unknown'/>
			</choices>
		</key>
		<key name="host-ip" type="s">
			<default>''</default>
		</key>
		<key name="dev-name" type="s">
			<default>''</default>
		</key>
		<key name="lab-name" type="s">
			<default>''</default>
		</key>
		<key name="allow-multiple-users" type="b">
			<default>false</default>
		</key>
	</schema>
</schemalist>
